package com.demo.beans;

public class Person {
	private int id;
	private String name;
	private String email;

	public Person() {
		this.id=0;
		this.name=null;
		this.email=null;
	}
	
	public Person(int id,String nm,String em) {
		this.id=id;
		this.name=nm;
		this.email=em;
	}
	
	public void setId(int id) {
		this.id=id;
	}
	
	public void setName(String nm) {
		this.name=nm;
	}
}
