package com.demo.pog1;

public class OccurancesOfCharinString {

	public static void main(String[] args) {
		static int count=0;
		String str="surajwable";
		
	}
	
	public static int count(String s,char c) {
		for(int i=0;i<s.length();i++) {
			if(charAt[i]==c) {
				count++;
			}
		}
		return i;
	}

}
