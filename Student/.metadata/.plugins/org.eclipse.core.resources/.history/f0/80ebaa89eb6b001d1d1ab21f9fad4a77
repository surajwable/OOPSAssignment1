
public class Student {
 private int sid;
 private String name;
 private int m1,m2,m3;
 static int pass_marks;
 static int count;
 
 static {
	 pass_marks=60;
	 count=0;
 }
 
 public Student() {
	 this.sid=++count;
	 this.name=null;
	 this.m1=0;
	 this.m2=0;
	 this.m3=0;
 }
 
 public Student(String nm,int m1,int m2,int m3) {
	 this.sid=++count+nm.substring(0,2);
	 this.name=nm;
	 
 }
}
