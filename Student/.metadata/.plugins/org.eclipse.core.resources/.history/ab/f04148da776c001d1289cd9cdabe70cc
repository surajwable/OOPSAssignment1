package com.demo.beans;

public class Customer extends Person {
	
	private String type;
	private String daddress;
	
	public Customer() {
		this.type=null;
		this.daddress=null;
	}
	
	public Customer(int id,String nm,String em,String type,String dadd) {
		super(id,nm,em);
		this.type=type;
		this.daddress=dadd;
	}
	
	
	

}
