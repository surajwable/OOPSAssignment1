package com.demo.beans;

import java.util.concurrent.Callable;

public class MyFactorial implements Callable{

	private int num;
	
	public MyFactorial(int num) {
		super();
		this.num = num;
	}

	@Override
	public Object call() throws Exception {
		int f=1;
		for(int i=2;i<=n;i++) {
			f=f*i;
		}
		System.out.println("Factorial of:"+n+"-->"+f);
		return null;
	}

	
}
