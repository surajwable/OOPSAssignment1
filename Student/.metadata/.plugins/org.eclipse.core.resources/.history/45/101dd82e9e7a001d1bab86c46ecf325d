package com.demo.beans;

public class MyDigitSum {

	private int num;

	public MyDigitSum(int num) {
		super();
		this.num = num;
	}
	
	public String call throws Exception{
		int f=1;
		for(int i=1;i<=n;i++) {
			f=f*i;
		}
		System.out.println("thread "+Thread.currentThread().getId()+"---factorial "+n);
		return "factorial of "+n+":  "+f;
	}
}
