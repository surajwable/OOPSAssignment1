package com.demo.beans;

public class SalariedEmp {
	
	private int sal;
	private int bonus;
	
	public SalariedEmp(){
		this.sal=0;
		this.bonus=0;
	}
	
	public SalariedEmp(int id,String nm,String em,String dept,String desg,int sal,int bonus) {
		super(id,nm,em,dept,desg);
		this.sal=sal;
		this.bonus=bonus;
	}

	public int getSal() {
		return sal;
	}

	public void setSal(int sal) {
		this.sal = sal;
	}

	public int getBonus() {
		return bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}

	public String toString() {
		return "SalariedEmp [sal=" + sal + ", bonus=" + bonus + "]";
	}
	

}
