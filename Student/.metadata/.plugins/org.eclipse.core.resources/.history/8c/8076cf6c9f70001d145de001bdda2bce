package com.demo.beans;

public class Product {
 
	private int id;
	private String name;
	private int qty;
	private float price;
	
	public Product() {
		super();
	}

	public Product(int id, String name, int qty, float price) {
		super();
		this.id = id;
		this.name = name;
		this.qty = qty;
		this.price = price;
	}
	
	
}
