package com.demo.beans;

public class MscStudent extends Student {
	
	private int m1,m2,m3;

	public MscStudent() {
		this.m1=0;
		this.m2=0;
		this.m3=0;
	}
	
	public MscStudent(int id,String nm,String adr,int m1,int m2,int m3) {
		super(id,nm,adr);
		this.m1=m1;
		this.m1=m2;
		this.m1=m3;
	}
	
	
}
