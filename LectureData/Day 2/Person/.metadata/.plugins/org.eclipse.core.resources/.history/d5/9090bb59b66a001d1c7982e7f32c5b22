
public class Person {
	private int perid;
	private String pname;
	private String mobile;
	
	//default constructor
	public Person() {
		this.perid=0;
		this.pname=null;
		this.mobile=null;
	}
	
	//parametrized constructor
	public Person(int id, String nm,String mob ) {
		this.perid=id;
		this.pname=nm;
		this.mobile=mob;
	}
	
	public String toString() {
		return "Id: "+this.perid +"Name: "+this.pname+"Mobile:"+this.mobile;
	}

}
